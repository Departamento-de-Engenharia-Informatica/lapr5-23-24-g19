@startuml
autonumber

actor "Campus Manager" as usr

participant "Campus Management" as mng

activate usr
    usr -> mng : list building floors with passage
    activate mng
        mng --> usr : show buildings and requests one
    deactivate mng

    usr -> mng : choses building
    activate mng
        mng --> usr : inform error/show floors
    deactivate mng
deactivate usr

@enduml
