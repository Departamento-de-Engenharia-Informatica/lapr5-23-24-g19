@startuml
autonumber

actor "Campus Manager" as usr

participant "Campus Management" as mng

activate usr
    usr -> mng : create building
    activate mng
        mng --> usr : request data
    deactivate mng

    usr -> mng : provides data
    activate mng
        mng --> usr : inform error/success
    deactivate mng
deactivate usr

@enduml
