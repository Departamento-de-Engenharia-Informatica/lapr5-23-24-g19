@startuml
autonumber

participant "<<System>>\nMD" as mdr

[-> mdr: PATCH /building/{id}/floors/{number}/map
activate mdr
alt floor not found
    [<--mdr  : 404: Not found
else map not processed
    [<--mdr : 422: Unprocessable Content
else floor map updated
    [<--mdr  : 201: Created
end
deactivate mdr

@enduml

