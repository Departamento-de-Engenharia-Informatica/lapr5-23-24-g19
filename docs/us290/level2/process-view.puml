@startuml
autonumber

participant "MD" as mdr




        [-> mdr: GET /buildings/{id}/elevators
        activate mdr
            alt building not found
                    [<-- mdr : 412: Precondition Failed
            else building found
                alt elevators not found
                    [<-- mdr : 412: Precondition Failed
                else elevators sent
                    [<-- mdr : 200: "OK" Successful
                end
            end
            deactivate mdr

    deactivate mdr


@enduml

