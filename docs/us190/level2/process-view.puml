@startuml
autonumber

participant "<<system>>\nMD" as mdr

[--> mdr: POST /building/{id}/floors

activate mdr
alt building not found
    [<--mdr : 404 not found
else floor already exists
    [<--mdr : 422 Unprocessable Content
else floor created successfully
    [<--mdr : 201 Created
end
deactivate mdr


@enduml
