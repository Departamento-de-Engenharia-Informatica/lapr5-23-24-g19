@startuml
autonumber

actor " " as actor
box "RobDroneIsep"
participant "MD" as mdr

activate actor

    actor -> mdr: POST /floor

    activate mdr
    alt building not found
        mdr --> actor : 412: Precondition Failed
    else floor created successfully
        mdr --> actor : 201: Created
    end
    deactivate mdr

deactivate actor

@enduml
