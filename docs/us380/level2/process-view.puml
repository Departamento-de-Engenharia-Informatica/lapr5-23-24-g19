@startuml
autonumber


participant "MD" as mdr


activate mdr

    [-> mdr: GET /robots
    alt unkown user
            [<-- mdr  : 401: Unauthorized
        else user not authorized
            [<-- mdr  : 403: Forbidden
        else user authorized


    alt robots not found
        [<-- mdr  : 412: Precondition Failed
    else robots sent
        [<-- mdr  : 200: "OK" Successful
    end

    end

deactivate mdr
@enduml
