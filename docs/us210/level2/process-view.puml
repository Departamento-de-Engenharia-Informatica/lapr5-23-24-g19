@startuml
autonumber

box "  <<mdrtem>>\nRobDroneGo"
participant "MD" as mdr

[-> mdr: GET /buildings/{id}/floors

activate mdr
alt building not found
    [<--mdr : response code: 422
else building found
    alt floors not found
        [<--mdr : response code: 422
    else floors found
        [<--mdr : response code: 200
    end
end
deactivate mdr

@enduml
